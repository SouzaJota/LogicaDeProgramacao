programa {
  funcao inicio() {
    real x, m, v, a, h
  escreva ("\n qual o valor de m: ")
  leia(m)
  escreva ("\n qual o valor de a: ")
  leia(a)
  escreva ("\n qual o valor de h: ")
  leia (h)
  escreva ("\n qual o valor de v: ")
  leia (v)
  
  x=m*(a*h)+(v*v/2)
  escreva ("\n o valor e: ",x)
  }
}
 